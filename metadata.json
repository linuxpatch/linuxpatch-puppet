{
  "name": "linuxpatch-linuxpatch_puppet",
  "version": "0.1.2",
  "author": "linuxpatch",
  "summary": "A Puppet module for LinuxPatch.com, a Patch Management platform. LinuxPatch.com offers automated Linux patch management, enabling secure, real-time updates with features like CVE reporting and Healthchecks. Free for up to 3 servers.",
  "license": "Apache-2.0",
  "source": "https://github.com/linuxpatch/linuxpatch-puppet",
  "project_page": "https://github.com/linuxpatch/linuxpatch-puppet",
  "issues_url": "https://github.com/linuxpatch/linuxpatch-puppet/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.25.0 < 7.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 6.0.0 < 8.0.0"
    }
  ],
  "operatingsystem_support": [
